  ],
  "name": "rust-ext",
  "description": "Compute ext over the Steenrod algebra",
  "version": "0.1.0",
  "scripts": {
    "build": "rimraf dist pkg && webpack --config webpack.config.js",
    "serve": "emrun --no_browser --port 8000 --serve_root dist .",
    "start": "rimraf dist pkg && webpack-dev-server --open -d",
    "test": "cargo test && wasm-pack test --headless"
  },
  "dependencies": {
    "spectral-sequences": "git+https://github.com/hoodmane/js_spectralsequences.git"
  },
  "devDependencies": {
    "@wasm-tool/wasm-pack-plugin": "^0.4.2",
    "copy-webpack-plugin": "^5.0.4",
    "rimraf": "^2.6.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.1"
  }
}
