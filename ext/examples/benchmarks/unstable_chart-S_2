\draw [grid] (6, 0) -- (6, 4);
\draw [major-grid] (0, 0) -- (6, 0);
\node [left] at (0, 0) {0};
\draw [grid] (0, 1) -- (6, 1);
\draw [grid] (0, 2) -- (6, 2);
\draw [grid] (0, 3) -- (6, 3);
\draw [major-grid] (0, 4) -- (6, 4);
\node [left] at (0, 4) {4};
\node [right] at (1, 3) {$S^0$};
\draw [fill] (0, 0) circle (0.1);
\end{tikzpicture}
\end{figure}
