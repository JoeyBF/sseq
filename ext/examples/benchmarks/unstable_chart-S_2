\node [below] at (0, 0) {0};
\draw [grid] (1, 0) -- (1, 4);
\draw [grid] (2, 0) -- (2, 4);
\draw [grid] (3, 0) -- (3, 4);
\draw [major-grid] (4, 0) -- (4, 4);
\node [below] at (4, 0) {4};
